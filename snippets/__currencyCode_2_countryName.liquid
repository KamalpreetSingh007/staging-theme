{%-liquid
  if currencyCode != blank
    case currencyCode
      when 'AED'
        echo 'United Arab Emirates'
      when 'AFN'
        echo 'Afghanistan'
      when 'ALL'
        echo 'ALBANIA'
      when 'AUD'
        echo 'Australia'
      when 'GBP'
        echo 'United Kingdom'
      when 'USD'
        echo 'United States'
      when 'NZD'
        echo 'New Zealand'
    endcase
  endif
-%}
